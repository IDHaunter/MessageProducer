COLUMN TABLE "Messages" (
     "ID" INTEGER GENERATED BY DEFAULT AS IDENTITY (NO CYCLE NO CACHE NO MINVALUE START WITH 1 INCREMENT BY 1 MAXVALUE 2999999999) NOT NULL COMMENT 'Message ID',
	 "CREATEDAT" TIMESTAMP COMMENT 'Message create timestamp',
	 "PROCESSEDAT" TIMESTAMP COMMENT 'Message processing end timestamp',
	 "TEXT" NVARCHAR(128) COMMENT 'Message text',
	 "IDSTATUS"INTEGER NOT NULL COMMENT 'Mesage status',
	 PRIMARY KEY ("ID"))
	 COMMENT 'Messages exchange history'
	 WITH ASSOCIATIONS( JOIN "Statuses" AS "S" ON "S"."ID" = "IDSTATUS")
UNLOAD PRIORITY 5 AUTO MERGE